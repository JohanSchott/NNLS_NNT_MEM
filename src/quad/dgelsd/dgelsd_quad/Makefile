F77 = gfortran
#F77 = ifort
F77FLAGS =  -O2

GENLIB= libdgelsdquad.a

   
# $(PROG): $(OBJS)

   
$(GENLIB): $(OBJS)
	ar r $(GENLIB) $(OBJS)

    #$(F90) $(LDFLAGS) -o $@ $(OBJS) $(LIBS)


.SUFFIXES: $(SUFFIXES) .f

.f.o:
	$(F77) $(F77FLAGS) -c $<



clean:
	rm -f $(GENLIB) $(OBJS) *.mod

